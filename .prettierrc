{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "es5",
  "importOrder": [
    "^(.*)(react)|(next)(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^(.*)components(.*)$",
    "^(.*)(actions)|(selectors)|(api)|(slice)$",
    "^(.*)types(.*)$",
    "^(.*).scss$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true
}
